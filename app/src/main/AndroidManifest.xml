<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.brainstorm.android.womenscalendar">
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM"/>
    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/app_icon_completed"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.note_redactor.activity.NoteRedactorActivity"></activity>
        <activity android:name=".presentation.statistics.activity.StatisticsActivity" />
        <activity android:name=".presentation.initialization.activity.InitializationActivity" />
        <activity android:name=".presentation.menu.activity.MenuActivity" />
        <activity android:name=".presentation.quiz.activity.QuizActivity" />
        <activity android:name=".presentation.splash.activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>